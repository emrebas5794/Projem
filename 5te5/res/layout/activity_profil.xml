<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
      android:layout_width="fill_parent"
    android:layout_height="match_parent"
    android:background="@drawable/futbol"
    tools:context="${relativePackage}.${activityClass}" >

    <ImageView
        android:id="@+id/profilimageView11"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:layout_below="@+id/profilbutton1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="53dp" />

    <Button
        android:id="@+id/profilbutton1"
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/profilimageView11"
        android:layout_alignParentTop="true"
        android:layout_marginTop="51dp"
        android:background="#1f1f1f"
        android:text="Sende Soru Yaz"
        android:textColor="#ffffff"
        android:textSize="20dp" />

    <Button
        android:id="@+id/imageView1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignBaseline="@+id/profilbutton1"
        android:layout_alignBottom="@+id/profilbutton1"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="13dp"
        android:background="@drawable/logo" />

    <Button
        android:id="@+id/profilbutton4"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignLeft="@+id/profilbutton3"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="17dp"
        android:background="#66000000"
        android:textColor="#FFFFFF"
       
        android:textSize="20dp" />

    <TextView
        android:id="@+id/haftatextView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView1"
        android:layout_alignBottom="@+id/textView1"
        android:layout_marginLeft="13dp"
        android:layout_toRightOf="@+id/imageView1"
        android:text="0"
         android:textSize="17dp"
        android:textColor="#FF0C00" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/haftatextView2"
        android:layout_alignBottom="@+id/haftatextView2"
        android:layout_marginLeft="13dp"
        android:layout_toRightOf="@+id/haftatextView2"
        android:text="5te5"
        android:textSize="17dp"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/imageView1"
        android:layout_alignTop="@+id/profilbutton4"
        android:layout_marginTop="20dp"
         android:textSize="17dp"
        android:text="Bu Hafta"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView1"
        android:layout_below="@+id/textView1"
        android:layout_marginTop="19dp"
        android:textSize="17dp"
        android:text="Toplam"
        android:textColor="#FFFFFF" />

    <TextView
        android:id="@+id/toplamtextView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView4"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/haftatextView2"
        android:text="0"
         android:textSize="17dp"
        android:textColor="#FF0C00"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/toplamtextView5"
        android:layout_alignBottom="@+id/toplamtextView5"
        android:layout_marginLeft="11dp"
        android:layout_toRightOf="@+id/toplamtextView5"
        android:text="5te5"
        android:textColor="#FFFFFF"
        android:textSize="17dp" />

    <Button
        android:id="@+id/pprofilbutton3"
       android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginLeft="50dp"
        android:layout_below="@+id/profilimageView11"
        android:layout_centerHorizontal="true"
       
        android:background="#1f1f1f"
        android:textColor="#FFFFFF"
        android:textSize="15dp" />

</RelativeLayout>
