                        switch (type) {
                            default:
                                break;
                            case "text":
                                {
                                    opts = [
                                        { text: "Eşit", value: "=" },
                                        { text: "Eşit Değil", value: "!=" },
                                        { text: "İçinde Geçen", value: "%" },
                                        { text: "İçinde Geçmeyen", value: "!%" }
                                    ];
                                }
                                break;
                            case "number":
                            case "date":
                                {
                                    opts = [
                                        { text: "Eşit ( = )", value: "=" },
                                        { text: "Büyük ( > )", value: ">" },
                                        { text: "Küçük ( < )", value: "<" },
                                        { text: "Büyük-Eşit ( >= )", value: ">=" },
                                        { text: "Küçük-Eşit ( <= )", value: "<=" }

                                    ];
                                }
                                break;
                            case "enum":
                            case "id":
                                {
                                    opts = [
                                        { text: "Eşit", value: "=" },
                                        { text: "Eşit Değil", value: "!=" }
                                    ]
                                }
                                break;
                        }
                        $.each(opts, function (i, v) {
                            $(el).closest(".form-row").find("td:nth-child(2) select").append($("<option>").text(v.text).attr("value", v.value));
                        });
